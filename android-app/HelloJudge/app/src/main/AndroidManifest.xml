        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login" />
        <activity android:name=".Qr_code" />
        <activity android:name=".Qr_code_One" />
        <activity android:name=".Qr_code_min" />
        <activity android:name=".Qr_code_One_min" />
        <activity android:name=".Judge" />
        <activity android:name=".Judge_One" />
        <activity android:name=".samp"></activity>
    </application>

</manifest>